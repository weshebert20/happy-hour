  <!-- searched portion -->

  <div class="container">
    <div class="row minOption">
      <div id="bodySecond">
        <div class="card-deck">
          <!-- begin card deck and loop through -->

          <% for(var i=0; i < found.length; i++) { %>
            <div class="card">

              <!-- <img class="card-img-top" src="<%= found[0].photo %>" alt="Card image cap"> -->
              <img class="card-img-top" src="<%= found[i].photo %>" alt="Card image cap">
                <div class="card-block cardNamePad">
                  <h4 class="card-title resName<%=[i]%>"><%= found[i].name%></h4>
                  <p class="idnums idNum<%=[i]%>"><%= found[i]._id%> </p>
                </div>
                <ul class="list-group list-group-flush">
                  <li class="list-group-item hours">Hour(s)- 

                      <!-- begin modal options -->              
                      <a class="clickModal" href="#close">
                          <i id="_id<%=[i]%>" class="fa hoursTimes fa-plus-circle modal-open" data-toggle="modal" data-target=".bd-example-modal-sm-_id<%=[i]%>"" aria-hidden="true"></i>
                          <div id="_id<%=[i]%>" class="modal fade bd-example-modal-sm-_id<%=[i]%>"" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel" aria-hidden="true">
                            <div class="modal-dialog modal-sm">
                              <div class="modal-content">
                                  <form action="" class="modalOptions formClose<%=[i]%>">
                                        <select class="custom-select mb-2 mr-sm-2 mb-sm-0" id="hoursFrom<%=[i]%>">
                                          <option selected>Choose...</option>
                                          <option value="3">3 p.m.</option>
                                          <option value="4">4 p.m.</option>
                                          <option value="5">5 p.m.</option>
                                        </select>
                                        <p class="timeP">To</p>
                                        <select class="custom-select mb-2 mr-sm-2 mb-sm-0" id="hoursTo<%=[i]%>">
                                          <option selected>Choose...</option>
                                          <option value="6">6 p.m.</option>
                                          <option value="7">7 p.m.</option>
                                          <option value="8">8 p.m.</option>
                                        </select>
                                        <input type="submit" 
                                       id="_id<%=[i]%>" class="btn btn-primary closeButton modalLoop hoursTime<%=[i]%>">
                                  </form>
                              </div>
                            </div>
                          </div>
                      </a>
                      <!-- modal end and hours/website begin -->   
                      <p class="madeHours clicked<%=[i]%> "><% found[i].hours %></p> 
                      <button type="submit" class="btn clickDel<%=[i]%> saveB btn-danger">Delete</button>
                  </li>
                  <li class="list-group-item rating">Rating: <%= found[i].rating.aggregate_rating%></li>
                </ul>
                <div class="card-block sites">
                  <a target="_blank" href=<%= found[i].url%> class="siteLink card-link">Website</a>
                  <a target="_blank" href="http://maps.google.com/?q= + <%= found[0].name%>" class="card-link"><i class="fa arrow fa-compass" aria-hidden="true"></i></a>
                </div>
                <div>
                <button type="submit" class="btn deleteMe saveBtn<%=[i]%> btnLog">Save</button>
                  
                </div>
              </div>
              <% } %>
              
  
  <!-- bootstrap set up -->

    <script
  src="https://code.jquery.com/jquery-3.2.1.min.js"
  integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4="
  crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4" crossorigin="anonymous"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/js/bootstrap.min.js" integrity="sha384-h0AbiXch4ZDo7tp9hKZ4TsHbi047NrKGLO3SEJAg45jXxnGIfYzk4Si90RDIqNm1" crossorigin="anonymous"></script>
    <script src="script.js"></script>
</body>
</html>