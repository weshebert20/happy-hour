                                          <option value="3">8</option>
                                        </select>
                                        <input type="submit" 
                                       id="hours" class="btn btn-primary hoursTime">
                                  </form>
                              </div>
                            </div>
                          </div>
                      </a>                    
                  </li>
                  <li class="list-group-item rating">Rating: <%= found[0].rating.aggregate_rating%></li>
                </ul>
