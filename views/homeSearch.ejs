  <div class="container">
    <div class="row minOption">
      <div id="bodySecond">
        <div class="card-deck">
          <% for(var i=0; i < found.length; i++) { %>
            <div class="card">
              <!-- <img class="card-img-top" src="<%= found[0].photo %>" alt="Card image cap"> -->
              <img class="card-img-top" src="<%= found[i].photo %>" alt="Card image cap">
                <div class="card-block cardNamePad">
                  <h4 class="card-title"><%= found[i].name%></h4>
                </div>
                <ul class="list-group list-group-flush">
                  <li class="list-group-item hours">Hour(s) -               
                      <a class="clickModal" href="#close">
                          <i class="fa hoursTimes fa-plus-circle modal-open" data-toggle="modal" data-target=".bd-example-modal-sm" aria-hidden="true"></i>
                          <div class="modal fade bd-example-modal-sm" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel" aria-hidden="true">
                            <div class="modal-dialog modal-sm">
                              <div class="modal-content">
                                  <form action="" class="modalOptions">
                                        <select class="custom-select mb-2 mr-sm-2 mb-sm-0" id="hoursFrom">
                                          <option selected>Choose...</option>
                                          <option value="3">3</option>
                                          <option value="4">4</option>
                                          <option value="5">5</option>
                                        </select>
                                        <p class="timeP">To</p>
                                        <select class="custom-select mb-2 mr-sm-2 mb-sm-0" id="hoursTo">
                                          <option selected>Choose...</option>
                                          <option value="6">6</option>
                                          <option value="7">7</option>
                                          <option value="8">8</option>
                                        </select>
                                        <input type="submit" 
                                       id="hours" class="btn btn-primary hoursTime">
                                  </form>
                              </div>
                            </div>
                          </div>
                      </a>   
                  

                      <p class="madeHours clicked<%=[i]%> "><% found[i].hours %></p>  
                  
                  </li>
                  <li class="list-group-item rating">Rating: <%= found[i].rating.aggregate_rating%></li>
                </ul>
                <div class="card-block sites">
                  <a target="_blank" href=<%= found[i].url%> class="card-link">Website</a>
                  <a target="_blank" href="http://maps.google.com/?q= + <%= found[0].name%>" class="card-link">Directions</a>
                </div>
              </div>
              <% } %>
              
  
  <!-- bootstrap set up -->

    <script
  src="https://code.jquery.com/jquery-3.2.1.min.js"
  integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4="
  crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4" crossorigin="anonymous"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/js/bootstrap.min.js" integrity="sha384-h0AbiXch4ZDo7tp9hKZ4TsHbi047NrKGLO3SEJAg45jXxnGIfYzk4Si90RDIqNm1" crossorigin="anonymous"></script>
    <script src="script.js"></script>
</body>
</html>