    <!-- Navigation -->
    <nav class="navbar navColor navbar-expand-lg fixed-top" id="mainNav">
      <div class="container">
        <a class="navbar-brand brand-name">Happy + Hour(s)</a>
        <div class="collapse navbar-collapse" id="navbarResponsive">
          <ul class="navbar-nav ml-auto">
            <li class="nav-item">
              <button type="button" class="btn btn-success">Log-Out</button>
