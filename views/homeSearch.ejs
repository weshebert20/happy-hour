    <!-- Page Header -->
    <header class="masthead headerBrand">
      <div class="container">
        <div class="row">
          <div class="col-lg-8 col-md-10 mx-auto">
            <h1 class="row about col-md-10">Find your happy place...</h1>
              <p class="zip">Enter your zipcode for top-rated places in your area</p>
              <form class="form-inline" id="nameEnter">
                <div class="form-group">
                 <div class="form-group mx-sm-3">
                   <input type="text" method="GET" class="form-control" id="inputName" placeholder="Enter zipcode...">
                 </div>
                 <input type="submit" class="btn searchButton btn-primary">
              </form>
          </div>
        </div>
      </div>
    </header>

